{"ast":null,"code":"export default {\n  name: 'PageHeader',\n  data() {\n    return {\n      localIp: '',\n      isp: '',\n      ipv6Support: false\n    };\n  },\n  mounted() {\n    this.getLocalIp();\n    this.checkIpv6Support();\n  },\n  methods: {\n    async getLocalIp() {\n      try {\n        const response = await fetch('http://localhost:8080/api/local-ip');\n        if (response.ok) {\n          const data = await response.json();\n          this.localIp = data.ip;\n          this.isp = data.isp;\n        }\n      } catch (error) {\n        console.error('获取IP失败:', error);\n      }\n    },\n    copyIp() {\n      if (this.localIp) {\n        const textarea = document.createElement('textarea');\n        textarea.value = this.localIp;\n        document.body.appendChild(textarea);\n        textarea.select();\n        try {\n          document.execCommand('copy');\n          this.$message.success('IP地址已复制');\n        } catch (err) {\n          this.$message.error('复制失败');\n        } finally {\n          document.body.removeChild(textarea);\n        }\n      }\n    },\n    async checkIpv6Support() {\n      try {\n        const useRealIp = localStorage.getItem('useRealIp') === 'true';\n        const realIp = localStorage.getItem('realIp');\n        const baseUrl = useRealIp && realIp ? `http://${realIp}:8080` : 'http://localhost:8080';\n        const response = await fetch(`${baseUrl}/api/check-ipv6`);\n        if (response.ok) {\n          const data = await response.json();\n          this.ipv6Support = data.supported;\n        }\n      } catch (error) {\n        console.error('检测IPV6支持失败:', error);\n        this.ipv6Support = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","localIp","isp","ipv6Support","mounted","getLocalIp","checkIpv6Support","methods","response","fetch","ok","json","ip","error","console","copyIp","textarea","document","createElement","value","body","appendChild","select","execCommand","$message","success","err","removeChild","useRealIp","localStorage","getItem","realIp","baseUrl","supported"],"sources":["src/components/Header.vue"],"sourcesContent":["<template>\n  <div class=\"header\">\n    <div class=\"logo\">\n      <i class=\"el-icon-video-camera\" style=\"font-size: 24px;\"></i>\n      <span class=\"title\">媒体流检测工具</span>\n    </div>\n    <div class=\"ip-info\" v-if=\"localIp\">\n      <el-tooltip effect=\"dark\" placement=\"bottom\">\n        <div slot=\"content\">\n          <div>本机IP地址</div>\n          <div>{{ isp }}</div>\n          <div>点击复制</div>\n        </div>\n        <el-tag \n          type=\"info\" \n          @click=\"copyIp\"\n          style=\"cursor: pointer\">\n          {{ isp }} - {{ localIp }}\n        </el-tag>\n      </el-tooltip>\n    </div>\n    <div class=\"header-right\">\n      <el-tooltip content=\"当前网络IPV6支持状态\" placement=\"bottom\">\n        <div class=\"ipv6-status\" :class=\"{ 'is-supported': ipv6Support }\">\n          <i :class=\"ipv6Support ? 'el-icon-check' : 'el-icon-close'\"></i>\n          <span>IPV6{{ ipv6Support ? '已支持' : '未支持' }}</span>\n        </div>\n      </el-tooltip>\n      <el-button\n          type=\"text\"\n          icon=\"el-icon-setting\"\n          @click=\"$router.push('/settings')\">\n        系统设置\n      </el-button>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'PageHeader',\n  data() {\n    return {\n      localIp: '',\n      isp: '',\n      ipv6Support: false\n    }\n  },\n  mounted() {\n    this.getLocalIp()\n    this.checkIpv6Support()\n  },\n  methods: {\n    async getLocalIp() {\n      try {\n        const response = await fetch('http://localhost:8080/api/local-ip')\n        if (response.ok) {\n          const data = await response.json()\n          this.localIp = data.ip\n          this.isp = data.isp\n        }\n      } catch (error) {\n        console.error('获取IP失败:', error)\n      }\n    },\n    copyIp() {\n      if (this.localIp) {\n        const textarea = document.createElement('textarea')\n        textarea.value = this.localIp\n        document.body.appendChild(textarea)\n        textarea.select()\n        \n        try {\n          document.execCommand('copy')\n          this.$message.success('IP地址已复制')\n        } catch (err) {\n          this.$message.error('复制失败')\n        } finally {\n          document.body.removeChild(textarea)\n        }\n      }\n    },\n    async checkIpv6Support() {\n      try {\n        const useRealIp = localStorage.getItem('useRealIp') === 'true'\n        const realIp = localStorage.getItem('realIp')\n        const baseUrl = useRealIp && realIp ? `http://${realIp}:8080` : 'http://localhost:8080'\n        \n        const response = await fetch(`${baseUrl}/api/check-ipv6`)\n        if (response.ok) {\n          const data = await response.json()\n          this.ipv6Support = data.supported\n        }\n      } catch (error) {\n        console.error('检测IPV6支持失败:', error)\n        this.ipv6Support = false\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n.header {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  padding: 0 24px;\n  background-color: #001529;\n  box-shadow: 0 1px 4px rgba(0,21,41,.08);\n}\n\n.logo {\n  display: flex;\n  align-items: center;\n}\n\n.title {\n  margin-left: 12px;\n  font-size: 18px;\n  font-weight: 400;\n  color: white;\n}\n\n.ip-info {\n  margin-left: auto;\n}\n\n.ip-info .el-tag {\n  background-color: #1890ff;\n  border-color: #1890ff;\n  color: white;\n  font-size: 14px;\n  height: 32px;\n  line-height: 30px;\n  padding: 0 15px;\n  border-radius: 2px;\n}\n\n.ip-info .el-tag:hover {\n  background-color: #40a9ff;\n  border-color: #40a9ff;\n  opacity: 1;\n}\n\n.header-right {\n  display: flex;\n  align-items: center;\n  gap: 15px;\n}\n\n.ipv6-status {\n  display: inline-flex;\n  align-items: center;\n  padding: 0 12px;\n  height: 24px;\n  line-height: 24px;\n  background: #f56c6c;\n  color: #fff;\n  border-radius: 12px;\n  font-size: 12px;\n  transition: all 0.3s ease;\n  cursor: default;\n  user-select: none;\n}\n\n.ipv6-status.is-supported {\n  background: #67c23a;\n}\n\n.ipv6-status i {\n  margin-right: 4px;\n  font-weight: bold;\n  font-size: 13px;\n}\n\n.ipv6-status span {\n  font-weight: 500;\n}\n</style> "],"mappings":"AAuCA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,OAAA;MACAC,GAAA;MACAC,WAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,UAAA;IACA,KAAAC,gBAAA;EACA;EACAC,OAAA;IACA,MAAAF,WAAA;MACA;QACA,MAAAG,QAAA,SAAAC,KAAA;QACA,IAAAD,QAAA,CAAAE,EAAA;UACA,MAAAV,IAAA,SAAAQ,QAAA,CAAAG,IAAA;UACA,KAAAV,OAAA,GAAAD,IAAA,CAAAY,EAAA;UACA,KAAAV,GAAA,GAAAF,IAAA,CAAAE,GAAA;QACA;MACA,SAAAW,KAAA;QACAC,OAAA,CAAAD,KAAA,YAAAA,KAAA;MACA;IACA;IACAE,OAAA;MACA,SAAAd,OAAA;QACA,MAAAe,QAAA,GAAAC,QAAA,CAAAC,aAAA;QACAF,QAAA,CAAAG,KAAA,QAAAlB,OAAA;QACAgB,QAAA,CAAAG,IAAA,CAAAC,WAAA,CAAAL,QAAA;QACAA,QAAA,CAAAM,MAAA;QAEA;UACAL,QAAA,CAAAM,WAAA;UACA,KAAAC,QAAA,CAAAC,OAAA;QACA,SAAAC,GAAA;UACA,KAAAF,QAAA,CAAAX,KAAA;QACA;UACAI,QAAA,CAAAG,IAAA,CAAAO,WAAA,CAAAX,QAAA;QACA;MACA;IACA;IACA,MAAAV,iBAAA;MACA;QACA,MAAAsB,SAAA,GAAAC,YAAA,CAAAC,OAAA;QACA,MAAAC,MAAA,GAAAF,YAAA,CAAAC,OAAA;QACA,MAAAE,OAAA,GAAAJ,SAAA,IAAAG,MAAA,aAAAA,MAAA;QAEA,MAAAvB,QAAA,SAAAC,KAAA,IAAAuB,OAAA;QACA,IAAAxB,QAAA,CAAAE,EAAA;UACA,MAAAV,IAAA,SAAAQ,QAAA,CAAAG,IAAA;UACA,KAAAR,WAAA,GAAAH,IAAA,CAAAiC,SAAA;QACA;MACA,SAAApB,KAAA;QACAC,OAAA,CAAAD,KAAA,gBAAAA,KAAA;QACA,KAAAV,WAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}