{"ast":null,"code":"export default {\n  name: 'PageHeader',\n  data() {\n    return {\n      localIp: '',\n      sourceCount: 0\n    };\n  },\n  mounted() {\n    this.getLocalIp();\n    this.$root.$on('update-source-count', this.updateSourceCount);\n  },\n  beforeDestroy() {\n    this.$root.$off('update-source-count', this.updateSourceCount);\n  },\n  methods: {\n    async getLocalIp() {\n      try {\n        // 获取本机IP\n        const response = await fetch('http://localhost:8080/api/local-ip');\n        if (response.ok) {\n          this.localIp = await response.text();\n        }\n      } catch (error) {\n        console.error('获取IP失败:', error);\n      }\n    },\n    copyIp() {\n      if (this.localIp) {\n        const textarea = document.createElement('textarea');\n        textarea.value = this.localIp;\n        document.body.appendChild(textarea);\n        textarea.select();\n        try {\n          document.execCommand('copy');\n          this.$message.success('IP地址已复制');\n        } catch (err) {\n          this.$message.error('复制失败');\n        } finally {\n          document.body.removeChild(textarea);\n        }\n      }\n    },\n    updateSourceCount(count) {\n      this.sourceCount = count;\n    }\n  }\n};","map":{"version":3,"names":["name","data","localIp","sourceCount","mounted","getLocalIp","$root","$on","updateSourceCount","beforeDestroy","$off","methods","response","fetch","ok","text","error","console","copyIp","textarea","document","createElement","value","body","appendChild","select","execCommand","$message","success","err","removeChild","count"],"sources":["src/components/Header.vue"],"sourcesContent":["<template>\n  <div class=\"header\">\n    <div class=\"logo\">\n      <i class=\"el-icon-video-camera\" style=\"font-size: 24px;\"></i>\n      <span class=\"title\">直播源检测工具</span>\n      <el-tag \n        type=\"success\" \n        size=\"small\"\n        class=\"source-count\">\n        直播源: {{ sourceCount }}\n      </el-tag>\n    </div>\n    <div 