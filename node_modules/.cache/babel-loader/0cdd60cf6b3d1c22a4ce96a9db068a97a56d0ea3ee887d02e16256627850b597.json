{"ast":null,"code":"export default {\n  name: 'StreamCheck',\n  data() {\n    return {\n      checkForm: {\n        url: '',\n        type: 'rtmp',\n        timeout: 5\n      },\n      checkResults: []\n    };\n  },\n  methods: {\n    startCheck() {\n      // 检测功能将在后续实现\n      this.$message.success('开始检测');\n    },\n    resetForm() {\n      this.checkForm = {\n        url: '',\n        type: 'rtmp',\n        timeout: 5\n      };\n    }\n  }\n};","map":{"version":3,"names":["name","data","checkForm","url","type","timeout","checkResults","methods","startCheck","$message","success","resetForm"],"sources":["src/components/StreamCheck.vue"],"sourcesContent":["<template>\r\n  <div class=\"stream-check\">\r\n    <el-card class=\"check-card\">\r\n      <div slot=\"header\">\r\n        <span>直播源检测</span>\r\n      </div>\r\n      \r\n      <el-form :model=\"checkForm\" label-width=\"100px\">\r\n        <el-form-item label=\"直播源地址\">\r\n          <el-input\r\n            v-model=\"checkForm.url\"\r\n            type=\"textarea\"\r\n            :rows=\"4\"\r\n            placeholder=\"请输入直播源地址，多个地址请换行输入\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        \r\n        <el-form-item label=\"检测类型\">\r\n          <el-radio-group v-model=\"checkForm.type\">\r\n            <el-radio label=\"rtmp\">RTMP</el-radio>\r\n            <el-radio label=\"hls\">HLS</el-radio>\r\n            <el-radio label=\"flv\">FLV</el-radio>\r\n          </el-radio-group>\r\n        </el-form-item>\r\n        \r\n        <el-form-item label=\"超时时间\">\r\n          <el-input-number \r\n            v-model=\"checkForm.timeout\" \r\n            :min=\"1\" \r\n            :max=\"60\"\r\n            :step=\"1\"\r\n          ></el-input-number>\r\n          <span class=\"unit\">秒</span>\r\n        </el-form-item>\r\n        \r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"startCheck\">开始检测</el-button>\r\n          <el-button @click=\"resetForm\">重置</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-card>\r\n    \r\n    <el-card class=\"result-card\" v-if=\"checkResults.length > 0\">\r\n      <div slot=\"header\">\r\n        <span>检测结果</span>\r\n        <el-button style=\"float: right; padding: 3px 0\" type=\"text\">\r\n          导出结果\r\n        </el-button>\r\n      </div>\r\n      \r\n      <el-table :data=\"checkResults\" style=\"width: 100%\">\r\n        <el-table-column prop=\"url\" label=\"直播源地址\" width=\"400\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"status\" label=\"状态\" width=\"100\">\r\n          <template slot-scope=\"scope\">\r\n            <el-tag :type=\"scope.row.status === '正常' ? 'success' : 'danger'\">\r\n              {{ scope.row.status }}\r\n            </el-tag>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"responseTime\" label=\"响应时间\" width=\"120\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"checkTime\" label=\"检测时间\">\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'StreamCheck',\r\n  data() {\r\n    return {\r\n      checkForm: {\r\n        url: '',\r\n        type: 'rtmp',\r\n        timeout: 5\r\n      },\r\n      checkResults: []\r\n    }\r\n  },\r\n  methods: {\r\n    startCheck() {\r\n      // 检测功能将在后续实现\r\n      this.$message.success('开始检测')\r\n    },\r\n    resetForm() {\r\n      this.checkForm = {\r\n        url: '',\r\n        type: 'rtmp',\r\n        timeout: 5\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.stream-check {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.check-card {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.unit {\r\n  margin-left: 10px;\r\n  color: #606266;\r\n}\r\n\r\n.result-card {\r\n  margin-top: 20px;\r\n}\r\n</style> "],"mappings":"AAsEA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;QACAC,GAAA;QACAC,IAAA;QACAC,OAAA;MACA;MACAC,YAAA;IACA;EACA;EACAC,OAAA;I